<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.orderingsystem.order.mapper.SendMapper">
    <insert id="addSend" keyColumn="sid" keyProperty="sid" useGeneratedKeys="true">
        insert into send(sid,deliver,dtime,uid)
        values (null,#{deliver},#{dtime},#{uid})
    </insert>
</mapper>
